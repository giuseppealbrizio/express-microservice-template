{
  "name": "node-express-rest-boilerplate",
  "version": "2.0.0",
  "description": "Node REST microservice boilerplate using node.js and express and some other cool stuff",
  "private": true,
  "license": "MIT",
  "bin": "src/bin/server.js",
  "main": "src/app.js",
  "author": {
    "name": "Giuseppe Albrizio",
    "email": "g.albrizio@gmail.com"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "ecma",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project",
    "eslint",
    "jest",
    "sendgrid",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/giuseppealbrizio/express-microservice-template.git"
  },
  "scripts": {
    "start:prod": "npm run prod",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf build",
    "transpile": "babel src --out-dir build",
    "server": "node ./build/bin/server",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/bin/server",
    "lint": "eslint -c './.eslintrc' --ignore-path .gitignore --ext .js .",
    "lint-and-fix": "eslint --ignore-path .gitignore --ext .js --fix .",
    "lint-all": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.1",
    "@google-cloud/storage": "^6.7.0",
