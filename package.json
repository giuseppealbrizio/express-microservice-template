{
  "name": "microservice-template",
  "version": "1.0.1",
  "description": "Microservice Template",
  "private": true,
  "license": "MIT",
  "bin": "src/bin/www.js",
  "main": "src/app.js",
  "author": {
    "name": "Giuseppe Albrizio",
    "email": "g.albrizio@gmail.com"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "ecma",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project",
    "eslint",
    "jest",
    "sendgrid",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/giuseppealbrizio/express-microservice-template.git"
  },
  "scripts": {
    "start:prod": "npm run prod",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf build",
    "transpile": "babel src --out-dir build",
    "server": "node ./build/bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/bin/www",
    "lint": "eslint -c './.eslintrc' --ignore-path .gitignore --ext .js .",
    "lint-and-fix": "eslint --ignore-path .gitignore --ext .js --fix .",
    "lint-all": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.5",
    "@google-cloud/storage": "^5.18.1",
    "@sendgrid/mail": "^7.6.0",
    "@skeldon/sdv3-shared-library": "^1.1.11",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "clean-deep": "^3.4.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "core-js": "^3.21.0",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.3.1",
    "mongoose": "^6.2.1",
    "mongoose-delete": "^0.5.4",
    "mongoose-slug-generator": "^1.0.4",
    "mongoose-type-url": "^2.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-cloud-storage": "^2.8.0",
    "mysql2": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.16.0",
    "slugify": "^1.6.5",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "twilio": "^3.73.1",
    "validator": "^13.7.0",
    "winston": "^3.5.1",
    "xmlbuilder2": "^3.0.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/core-js": "^2.5.5",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
