    "mysql2": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.7",
    "regenerator-runtime": "^0.13.11",
    "sequelize": "^6.26.0",
    "slugify": "^1.6.5",
