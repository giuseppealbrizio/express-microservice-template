    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.3.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "messagebird": "^3.8.1",
    "moment": "^2.29.4",
    "mongodb": "^4.12.1",
    "mongoose": "^6.7.5",
    "mongoose-delete": "^0.5.4",
    "mongoose-type-url": "^2.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
